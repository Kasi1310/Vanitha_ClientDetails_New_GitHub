<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="frmClientReview.aspx.cs" Inherits="ClientDetails.frmClientReview" EnableEventValidation="false"  %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Client Review Form</title>
    <script src="Scripts/jquery-3.0.0.min.js"></script>
    <script src="Scripts/bootstrap.min.js"></script>
    <link href="Content/bootstrap.css" rel="stylesheet" />
    <script src="Scripts/Custom/Custom.js"></script>

    <link href="Assets/css/flatpickr.min.css" rel="stylesheet" />
    <link href="Assets/css/material_green.css" rel="stylesheet" />
    <script src="Assets/js/flatpickr.min.js"></script>
    <script src="Assets/js/ClientReviewFormJS/getClientReviewDetail.js"></script>
       
   
    <style>
        /* [Previous CSS styles remain exactly the same] */
        body, html {
        height: 100%;
    }

    .border-10 {
        border: 3pt solid #009094;
        border-style: solid;
        border-width: 2px;
    }
    :root {
        --primary-color: #4361ee;
        --secondary-color: #3a0ca3;
        --accent-color: #f72585;
        --light-color: #f8f9fa;
        --medium-gray: #e9ecef;
        --dark-gray: #495057;
        --success-color: #4cc9f0;
        --warning-color: #f8961e;
        --info-color: #4895ef;
        --border-radius: 10px;
        --box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
    }

    body {
        font-family: Calibri;
        font-size: 16px;
        background: linear-gradient(135deg, #f5f7fa 0%, #e4e8f0 100%);
        padding: 20px;
        color: var(--dark-gray);
        line-height: 1;
        min-height: 100vh;
    }

    .form-container {
        max-width: 1200px;
        margin: 20px auto;
        background-color: white;
        padding: 30px 40px;
        border-radius: 10px;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
        border: 1px solid rgba(255, 255, 255, 0.3);
    }



    .form-header {
        text-align: center;
        margin-bottom: 30px;
        padding-bottom: 15px;
        background: linear-gradient(to right, var(--primary-color), var(--secondary-color));
        color: white;
        padding: 20px;
        border-radius: var(--border-radius);
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    }

    .form-header h1 {
        font-weight: 700;
        margin-bottom: 5px;
        letter-spacing: 0.5px;
        text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
    }

    .section:hover {
        transform: translateY(-3px);
        box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
    }

    

    #epcr-title{
        text-transform: none;
    }

 

    .form-table {
        width: 100%;
        margin-bottom: 1rem;
        color: var(--dark-gray);
        border-collapse: separate;
        border-spacing: 0 12px;
    }

    .form-table td {
        padding: 15px 20px;
        vertical-align: middle;
    }

    .form-table tr {
        background-color: rgba(248, 249, 250, 0.7);
        transition: all 0.3s ease;
        border-radius: var(--border-radius);
        box-shadow: 0 2px 5px rgba(0, 0, 0, 0.03);
    }

    .form-table tr:hover {
        background-color: rgba(67, 97, 238, 0.05);
        box-shadow: 0 5px 15px rgba(67, 97, 238, 0.1);
    }

    .form-label {
        font-weight: 600;
        margin-bottom: 0px;
        color: rgb(93,103,112);
        white-space: normal;
        /*display: flex;
        align-items: center;*/        
        gap: 5px;
    }


    .flatpickr {
        background-color: rgba(255, 255, 255, 0.8);
    }

    .form-control {
        width: 100%;
        padding: 7px 10px 6px 6px;
        /*padding: 12px 15px;*/
        border: 2px solid #e2e8f0;
        border-radius: var(--border-radius);
        transition: all 0.3s ease;
        font-size: 0.95rem;
        background-color: rgba(255, 255, 255, 0.8);
    }

     .form-control-nontransport {
     width: 100%;
     padding: 6%;
     /*padding: 12px 15px;*/
     border: 2px solid #e2e8f0;
     border-radius: var(--border-radius);
     transition: all 0.3s ease;
     font-size: 0.95rem;
     background-color: rgba(255, 255, 255, 0.8);
 }

    .form-control:focus {
        border-color: rgb(93,103,112);
        box-shadow: 0 0 0 3px rgba(67, 97, 238, 0.2);
        outline: none;
        background-color: white;
    }

    .radio-group {
        display: flex;
        gap: 25px;
        align-items: center;
    }

    .radio-option {
        display: flex;
        align-items: center;
        position: relative;
    }


    .form-check-label {
        font-weight: 500;
        color: var(--dark-gray);
        cursor: pointer;
        transition: color 0.2s ease;
    }

    .form-check-input:checked ~ .form-check-label {
        color: var(--secondary-color);
        font-weight: 600;
    }

    .btn-submit {
        padding: 14px 35px;
        font-weight: 600;
        letter-spacing: 0.5px;
       /* background: linear-gradient(to right, rgb(101,203,201), rgb(00,175,169));*/
        border: none;
        border-radius: 50px;
        transition: all 0.3s ease;
        font-size: 1rem;
        color: white;
        text-transform: uppercase;
        box-shadow: 0 4px 15px rgba(67, 97, 238, 0.3);
        position: relative;
        overflow: hidden;
    }

    .btn-submit:hover {
        transform: translateY(-3px);
        box-shadow: 0 8px 25px rgba(67, 97, 238, 0.4);
       background: linear-gradient(to right, rgb(00,175,169), rgb(101,203,201));
    }

    .btn-submit:active {
        transform: translateY(1px);
    }

    .validation-error {
        color: var(--accent-color);
        font-size: 0.85rem;
        margin-top: 5px;
        display: block;
        font-weight: 500;
        padding-left: 5px;
    }

    .input-group {
        display: flex;
        align-items: center;
        border-radius: var(--border-radius);
        overflow: hidden;
    }

    .input-group-text {
        display:none;
        min-width: 45px;
        justify-content: center;
        background-color: var(--primary-color);
        border: none;
        padding: 12px 15px;
        color: white;
        font-weight: 600;
    }

    .input-group .form-control {
        border-top-left-radius: 0;
        border-bottom-left-radius: 0;
        border-left: none;
    }

    .input-group-text:first-child {
        border-top-right-radius: 0;
        border-bottom-right-radius: 0;
    }

    .form-footer {
        text-align: center;
        margin-top: 40px;
        padding-top: 20px;
        border-top: 1px dashed var(--medium-gray);
    }

    .alert {
        padding: 15px 20px;
        margin-bottom: 20px;
        border-radius: var(--border-radius);
        border-left: 5px solid;
    }

    .alert-danger {
        background-color: #fff5f7;
        border-color: var(--accent-color);
        color: #721c24;
    }    

    .datepicker{
        background-color:#ffffff !important;
    }

    /* Responsive adjustments */
    @media (max-width: 768px) {
        .form-container {
            padding: 20px;
        }
        
        .form-table td {
            display: block;
            width: 100%;
            padding: 12px 15px;
        }
        
        .form-table tr {
            margin-bottom: 15px;
            display: block;
        }
        
        .radio-group {
            flex-direction: column;
            align-items: flex-start;
            gap: 12px;
        }
        
        .section {
            padding: 20px;
        }
        
        .form-label::before {
            display: none;
        }
        
        .section-title::after {
            display: none;
        }
    }
    .underline-textbox {
        border: none;
        border-bottom: 2px solid #000; /* Change color and thickness as needed */
        border-radius: 0;
        outline: none;
        box-shadow: none;
        background-color: transparent;
    }
    .underline-textbox:focus {
        border-bottom: 2px solid #007bff; /* or any color you prefer */
    }

    /* Animation for form loading */
    @keyframes fadeIn {
        from { opacity: 0; transform: translateY(20px); }
        to { opacity: 1; transform: translateY(0); }
    }

    .form-container {
        animation: fadeIn 0.6s ease-out forwards;
    }

    /* Hover effects for labels */
    .form-label:hover {
        color: rgb(93,103,112);
    }

    .form-label:hover::before {
        transform: scale(1.3);
    }

    </style>

</head>
<body>   
    <!-- All your client review form contents go here -->
        <form id="form1" runat="server"> 
            <div id="loader" style="display:none; position:fixed; top:0; left:0; right:0; bottom:0;
                 background: rgba(0,0,0,0.5); z-index:9999; align-items:center; justify-content:center;">
                <div class="spinner-border text-light" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
            </div>
           
            <asp:ScriptManager ID="ScriptManager1" runat="server" />
            <div class="form-container border-10">
                <div style="text-align:center;background: white;">
                    <img src="data:image/jpg;base64," style="height:70px;width:auto" alt="Logo" />
                </div>
                <h1 class="text-center mb-4" style="font-family: Calibri; font-size: 40px; font-weight: bold; color: #009094; text-align: center;">CLIENT REVIEW</h1>
              

                <!-- Meeting Details Section -->
                <div class="section" style="margin-bottom: 35px;padding: 25px;background-color: #fff;border-radius: 10px;box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);border-left: 5px solid rgb(00,148,144);transition: transform 0.3s ease, box-shadow 0.3s ease;">
                    <h3 class="section-title" style="font-weight: 700;color: rgb(00,175,169);margin-bottom: 25px;padding-bottom: 10px;border-bottom: 2px solid rgb(101,203,201);font-size: 1.3rem;display: flex;align-items: center;counter-increment: section;">MEETING DETAILS</h3>

                    <table style="width: 100%; border-spacing: 0 10px;">
                        <!-- Row 1 -->
                        <tr style="text-align:center !important;">
                            <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">CLIENT NUMBER#:</label>
                                    <asp:DropDownList ID="ddlClientNumber" runat="server" CssClass="form-control search-dropdown" AppendDataBoundItems="true" AutoPostBack="true" OnSelectedIndexChanged="ddlClientNumber_SelectedIndexChanged">                                        
                                    </asp:DropDownList>                                  
                                </div>
                            </td>
                             <%-- <asp:TextBox ID="txtClientNumber" runat="server" CssClass="form-control" placeholder="Enter Client ID"></asp:TextBox>--%>
                            <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">CLIENT NAME:</label>
                                    <asp:TextBox ID="txtClientName" runat="server" CssClass="form-control"></asp:TextBox>
                                </div>
                            </td>
                            <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">MEETING OR SENT DATE:</label>
                                    <asp:TextBox ID="txtMeetingDate" runat="server" CssClass="form-control datepicker"></asp:TextBox>
                                </div>
                            </td>
                            
                        </tr>

                        <!-- Row 2 -->
                        <tr style="text-align:center !important;">
                                <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">ACCOUNT EXECUTIVE:</label>
                                    <asp:TextBox ID="txtAccountExecutive" runat="server" CssClass="form-control"></asp:TextBox>
                                </div>
                            </td>
                            <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">EMAIL:</label>
                                    <asp:TextBox ID="txtEmail" runat="server" TextMode="Email" CssClass="form-control"></asp:TextBox>
                                </div>
                            </td>
                            <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">PHONE #:</label>
                                    <asp:TextBox ID="txtPhone" runat="server" TextMode="Phone" CssClass="form-control"></asp:TextBox>
                                </div>
                            </td>
                           
                        
                        </tr>

                        <!-- Row 3 -->
                        <tr style="text-align:center !important;">
                                <td style="display: inline-block; width: 33.33%; ">
                            <div class="form-group">
                                <label class="form-label">RECIPIENT RECEIVING NAME:</label>
                                <asp:TextBox ID="txtRecipientReceivingName" runat="server" CssClass="form-control"></asp:TextBox>
                            </div>
                        </td>
                            <td style="display: inline-block; width: 33.33%; ">
                            <div class="form-group">
                                <label class="form-label">RECIPIENT RECEIVING TITLE:</label>
                                <asp:TextBox ID="txtRecipientReceivingTitle" runat="server" CssClass="form-control"></asp:TextBox>
                            </div>
                        </td>
                            <td style="display: inline-block; width: 33.33%; ">
                                <div class="form-group">
                                    <label class="form-label">RECIPIENT RECEIVING EMAIL:</label>
                                    <asp:TextBox ID="txtRecipientReceivingEmail" runat="server" TextMode="Email" CssClass="form-control"></asp:TextBox>
                                </div>
                            </td>
                            
                        </tr>
                        
                    </table>
                </div>

                <!-- RPT and Collection Rates Section -->
                <div class="section" style="margin-bottom: 35px;padding: 25px;background-color: #fff;border-radius: 10px;box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);border-left: 5px solid rgb(00,148,144);transition: transform 0.3s ease, box-shadow 0.3s ease;">
        <h3 class="section-title" style="font-weight: 700;color: rgb(00,175,169);margin-bottom: 25px;padding-bottom: 10px;border-bottom: 2px solid rgb(101,203,201);font-size: 1.3rem;display: flex;align-items: center;counter-increment: section;">
            PERFORMANCE METRICS
        </h3>
      
        <table style="width: 100%; border-spacing: 0 10px;">
            <!-- Row 1 -->
            <tr style="text-align:center !important;">
                <td style="display: inline-block; width: 33.33%; ">
                <div class="form-group">
                    <label class="form-label">CLIENT REVENUE NUMBERS - START DATE:</label>
                    <asp:TextBox ID="txtClientRevenueNumberStartDate" runat="server"   CssClass="form-control datepicker"></asp:TextBox>
                   
                </div>
            </td>
            <td style="display: inline-block; width: 33.33%; ">
                <div class="form-group">
                    <label class="form-label">CLIENT REVENUE NUMBERS - END DATE:</label>
                    <asp:TextBox ID="txtClientRevenueNumberEndDate" runat="server"  CssClass="form-control datepicker" AutoPostBack="true" OnTextChanged="txtClientRevenueNumberEndDate_TextChanged" 
                         onchange="showLoader();"></asp:TextBox>
                </div>
            </td>
               
            <td style="display: inline-block; width: 33.33%; ">
                <div class="form-group">
                    <label class="form-label">REPORT TYPE</label>
        
                    <asp:TextBox ID="txtReportType" runat="server" CssClass="form-control" Text="Date of Entry" Enabled="false"></asp:TextBox>
                </div>
            </td>
            </tr>
            <tr style="line-height:1 !important; text-align:center;">
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                    <div class="form-group">
                        <label class="form-label" style="padding-top:20% !important">TRANSPORTS:</label>
                        <asp:TextBox ID="txtTransports" runat="server" CssClass="form-control"></asp:TextBox>
                    </div>
                </td>
                <td hidden="hidden" style="display:inline-block;width: 14.28%;vertical-align: top;">
                    <div class="form-group">
                        <label class="form-label" style="padding-top:20% !important">YTD REVENUE:</label>
                        <asp:TextBox ID="txtYtdRevenue" runat="server" CssClass="form-control"></asp:TextBox>
                    </div>
                </td>
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                    <div class="form-group">
                        <label style="padding-top:10% !important;" class="form-label">REVENUE PER TRANSPORT:</label>
                        <asp:TextBox ID="txtRevenuePerTransport" runat="server" CssClass="form-control"></asp:TextBox>
                    </div>
                </td>
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                <div class="form-group">
                    <label class="form-label" style="padding-top:20% !important">CHARGES:</label>
                    <asp:TextBox ID="txtCharges" runat="server" CssClass="form-control" ></asp:TextBox>
                </div>
                </td>
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                <div class="form-group">
                    <label class="form-label" style="padding-top:20% !important">PAYMENTS:</label>
                    <asp:TextBox ID="txtPayments" runat="server" CssClass="form-control"></asp:TextBox>
                </div>
                </td>
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                <div class="form-group">
                    <label class="form-label" style="padding-top:20% !important">ADJUSTMENTS:</label>
                    <asp:TextBox ID="txtAdjustments" runat="server" CssClass="form-control"></asp:TextBox>
                </div>
                </td>
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                    <div class="form-group">
                        <label class="form-label" style="padding-top:20% !important">WRITE-OFFS:</label>
                        <asp:TextBox ID="txtWriteOffs" runat="server" CssClass="form-control"></asp:TextBox>
                    </div>
                </td>
                <td style="display:inline-block;width: 14.28%;vertical-align: top;">
                    <div class="form-group">
                        <label class="form-label" style="padding-top:20% !important;">COLLECTION RATE %:</label>
                        <asp:TextBox ID="txtCollectionRate" runat="server" CssClass="form-control"></asp:TextBox>
                    </div>
                </td>
                    <td style="display:inline-block;width: 50%;vertical-align: top;">
                    <div class="form-group">
                        <label class="form-label">CLIENT COMMENTS:</label>
                        <asp:TextBox ID="txtClientComments" TextMode="MultiLine" runat="server" CssClass="form-control"></asp:TextBox>
                    </div>
                </td>
                <td style="display:inline-block;width: 50%;vertical-align: top;">
                <div class="form-group">
                    <label class="form-label">AE COMMENTS:</label>
                    <asp:TextBox ID="txtAEComments" TextMode="MultiLine" runat="server" CssClass="form-control" ></asp:TextBox>
                </div>
               </td>
                    
            </tr>

        </table>
    </div>

              
            
                <!-- Billing Rate Review Section -->
                <div class="section" style="margin-bottom: 35px;padding: 25px;background-color: #fff;border-radius: 10px;box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);border-left: 5px solid rgb(00,148,144);transition: transform 0.3s ease, box-shadow 0.3s ease;">
     <h3 class="section-title" style="font-weight: 700;color: rgb(00,175,169);margin-bottom: 25px;padding-bottom: 10px;border-bottom: 2px solid rgb(101,203,201);font-size: 1.3rem;display: flex;align-items: center;counter-increment: section;">
         YOUR CHARGE RATES
     </h3>

     <table style="width: 100%; border-spacing: 0 10px;">
         <!-- Row 1 -->
         <tr style="text-align:center !important;">
             <td style="display:inline-block;width: 12.50%;vertical-align: top;">
                 <div class="form-group">
                     <label class="form-label" style="padding-top:10% !important;">DATE OF LAST RATE CHANGE:</label>
                     <asp:TextBox ID="txtDateOfLastRateChange" runat="server" TextMode="Date" CssClass="form-control datepicker"></asp:TextBox>
                 </div>
             </td>
             <td style="display:inline-block;width: 12.50%;vertical-align: top;">
                 <div class="form-group">
                     <label class="form-label" style="padding-top:22% !important">BLS:</label>
                     <asp:TextBox ID="txtBls" runat="server" CssClass="form-control"></asp:TextBox>
                 </div>
             </td>
             <td style="display:inline-block;width: 12.50%;vertical-align: top;">
                 <div class="form-group">
                     <label class="form-label" style="padding-top:22% !important">BLS NE:</label>
                     <asp:TextBox ID="txtBlsNe" runat="server" CssClass="form-control"></asp:TextBox>
                 </div>
             </td>

             <td style="display:inline-block;width: 12.50%;vertical-align: top;">
             <div class="form-group">
                 <label class="form-label" style="padding-top:22% !important">ALS:</label>
                 <asp:TextBox ID="txtAls" runat="server" CssClass="form-control"></asp:TextBox>
             </div>
             </td>
              <td style="display:inline-block;width: 12.50%;vertical-align: top;">
                  <div class="form-group">
                      <label class="form-label" style="padding-top:22% !important">ALS NE:</label>
                      <asp:TextBox ID="txtAlsNe" runat="server" CssClass="form-control"></asp:TextBox>
                  </div>
              </td>
              <td style="display:inline-block;width: 12.50%;vertical-align: top;">
                  <div class="form-group">
                      <label class="form-label" style="padding-top:22% !important">ALS2:</label>
                      <asp:TextBox ID="txtAls2" runat="server" CssClass="form-control"></asp:TextBox>
                  </div>
              </td>
             <td style="display:inline-block;width: 12.50%;vertical-align: top;" colspan="1">
             <div class="form-group">
                 <label class="form-label" style="padding-top:22% !important">MILEAGE:</label>
                 <asp:TextBox ID="txtMileage" runat="server" CssClass="form-control"></asp:TextBox>
             </div>
         </td>
             <td style="display:inline-block;width: 12.50%;vertical-align: top; padding-top:3% !important;" colspan="1">
             <asp:UpdatePanel ID="upNonTransportRadios" runat="server" UpdateMode="Conditional">
                <ContentTemplate>
                    <div class="form-group text-center">
                        <label class="form-label d-block mb-2">NON-TRANSPORT:</label>
                        <div class="radio-group d-flex justify-content-center align-items-center">                            
                                <div class="form-check me-3 d-flex align-items-center">
                                    <asp:RadioButton ID="rbNonTransportYes" runat="server" GroupName="NonTransport"
                                        CssClass="form-check-input" AutoPostBack="true" OnCheckedChanged="toggleNonTransportComment" />
                                    <label class="form-check-label" for="<%= rbNonTransportYes.ClientID %>">Yes</label>&nbsp;&nbsp;&nbsp;
                                </div>
                                <div class="form-check me-3 d-flex align-items-center">
                                    <asp:RadioButton ID="rbNonTransportNo" runat="server" GroupName="NonTransport"
                                        CssClass="form-check-input" AutoPostBack="true" OnCheckedChanged="toggleNonTransportComment" />
                                    <label class="form-check-label" for="<%= rbNonTransportNo.ClientID %>">No</label>
                                </div>                            
                        </div>
                    </div>
                </ContentTemplate>
            </asp:UpdatePanel>
         </td>
             <td hidden="hidden" style="display:inline-block;width: 11.11%;vertical-align: top;" colspan="1">
             <asp:UpdatePanel ID="upNonTransportComment" runat="server" UpdateMode="Conditional">
                <ContentTemplate>
                    <div class="form-group">
                        <label id="lblNontransportComments" runat="server" Visible="false" class="form-label">NON TRANSPORT COMMENTS:</label>
                        <asp:TextBox ID="txtNonTransportComment" runat="server" Visible="false" CssClass="form-control" style="margin-top: 45% !important;" ></asp:TextBox>
                    </div>
                </ContentTemplate>
            </asp:UpdatePanel>
         </td>
         </tr>

         <!-- Row 2 -->
          <tr style="text-align:center !important;">
              <td style="display:inline-block;width: 22%;vertical-align: top;" colspan="1">
                     <div class="form-group text-center">
                         <label class="form-label d-block mb-2">DO YOU WANT TO MAKE ANY CHANGES IN THE ABOVE RATES:</label>
