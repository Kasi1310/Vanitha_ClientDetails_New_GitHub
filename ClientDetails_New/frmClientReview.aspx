        function populateReviewForm(data) {
            if (!data) return;

            Object.keys(data).forEach(function (key) {
                var value = data[key] ?? '';
                var $element = $('#' + key);

                // Special handling for recipients array
                if (key === 'recipients' && Array.isArray(value)) {
                    // Clear existing recipient rows except the first one
                    // Assuming rows have a class 'recipientRow'
                    var $container = $('#recipientContainer');
                    $container.find('.recipientRow').not(':first').remove();

                    // Populate the first recipient row
                    if (value.length > 0) {
                        var firstRecipient = value[0];
                        var $firstRow = $container.find('.recipientRow:first');

                        $firstRow.find('input[name="recipientReceivingName"]').val(firstRecipient.recipientReceivingName || '');
                        $firstRow.find('input[name="recipientReceivingTitle"]').val(firstRecipient.recipientReceivingTitle || '');
                        $firstRow.find('input[name="recipientReceivingEmail"]').val(firstRecipient.recipientReceivingEmail || '');
                    }

                    // Add extra recipient rows if more than one
                    for (let i = 1; i < value.length; i++) {
                        var recipient = value[i];
                        // Clone first row
                        var $newRow = $container.find('.recipientRow:first').clone();

                        // Clear validation messages if any (optional)
                        $newRow.find('span').hide();

                        // Fill values
                        $newRow.find('input[name="recipientReceivingName"]').val(recipient.recipientReceivingName || '');
                        $newRow.find('input[name="recipientReceivingTitle"]').val(recipient.recipientReceivingTitle || '');
                        $newRow.find('input[name="recipientReceivingEmail"]').val(recipient.recipientReceivingEmail || '');

                        $newRow.find('label').remove();
                        // Append cloned row
                        $container.append($newRow);
                    }

                    return; // skip normal processing for this key
                }

                // Handle radio buttons
                var $radioGroup = $('input[type="radio"][name="' + key + '"]');
                if ($radioGroup.length > 0) {
                    $radioGroup.each(function () {
                        if ($(this).val() === value) {
                            $(this).prop('checked', true);
                            $(this).trigger('change'); // Trigger change for dependent logic
                        }
                    });
                    return; // Skip to next item after handling radio
                }

                // Handle other types
                if ($element.length > 0) {
                    if ($element.is('input') || $element.is('textarea') || $element.is('select')) {
                        $element.val(value).trigger('change'); // trigger change if needed
                    } else {
                        $element.text(value);
                    }
                }
            });

            updateRecipientButtons();
            // Set formType based on input (if available), fallback to "NEW-0"
            $('#formType').val(formType || 'NEW-0');
        }


        $(document).ready(function () {
            // Apply flatpickr to ALL .datepicker fields
            $(".datepicker").flatpickr({
                dateFormat: "m/d/Y",  // MM/DD/YYYY
                maxDate: "today",     // prevent future dates
                allowInput: true
            });

            $(".nextReviewDatepicker").flatpickr({
                dateFormat: "m/d/Y",  // MM/DD/YYYY
                allowInput: true
            });

            // Specific fields for date range logic
            const $from = $('#clientRevenueNumberStartDate');
            const $to = $('#clientRevenueNumberEndDate');

            // Initialize To Date with reference
            const toPicker = $to.flatpickr({
                dateFormat: "m/d/Y",
                maxDate: "today",
                allowInput: true,
                onChange: function () {
                    $("#dropdownSelect").val("");
                }
            });

            // Re-initialize From Date to control minDate logic on To Date
            $from.flatpickr({
                dateFormat: "m/d/Y",
                maxDate: "today",
                allowInput: true,
                onChange: function (selectedDates, dateStr, instance) {
                    $("#dropdownSelect").val("");

                    if (selectedDates.length > 0) {
                        const fromDate = selectedDates[0];

                        // Set minDate on To date picker
                        toPicker.set('minDate', fromDate);

                        // Clear To Date if before new From Date
                        const currentToDate = toPicker.selectedDates[0];
                        if (currentToDate && currentToDate < fromDate) {
                            $to.val('');
                            toPicker.clear();
                        }
                    } else {
                        // If From Date cleared, reset minDate on To
                        toPicker.set('minDate', null);
                    }
                }
            });

            // Optional: Set placeholder text for all datepickers
            //$(".datepicker").attr("placeholder", "MM/DD/YYYY");
            //$(".nextReviewDatepicker").attr("placeholder", "MM/DD/YYYY");


            if (typeof reviewData !== 'undefined' && reviewData !== null) {
                populateReviewForm(reviewData);
            } else {
                // No reviewData: fresh form load
                $('#formType').val(formType || 'NEW-0');
            }

            // Initialize toggles
            toggleGroup('rbChiefCorrectYes', 'rbChiefCorrectNo', ['chiefNameGroup', 'chiefEmailGroup', 'chiefPhoneGroup']);
            toggleGroup('rbFiscalCorrectYes', 'rbFiscalCorrectNo', ['fiscalNameGroup', 'fiscalEmailGroup', 'fiscalPhoneGroup']);
