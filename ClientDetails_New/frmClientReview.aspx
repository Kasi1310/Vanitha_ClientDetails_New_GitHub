        $(document).ready(function () {
            $(".datepicker").flatpickr({
                dateFormat: "m-d-Y",
                placeholder: 'MM/DD/YYYY'
            });

        })

        function AllDateFormat() {
            const dateLabels = [
                'txtMeetingDate',
                'txtRevenueStartDate',
                'txtRevenueEndDate',
                'txtRenewalDate',
                'txtNextReviewDate',
                'txtReconciliationDate',
                'txtDOSE'

            ];

            dateLabels.forEach(labelId => {
                const label = document.getElementById(labelId);
                if (label && label.textContent && label.textContent.trim() !== "") {
                    label.textContent = formatDate(label.textContent);
                }
            });
        }

        function formatDate(dateString) {
            dateString = dateString.replace(/[.\s/]/g, "-");
            const dateParts = dateString.split("-");
            if (dateParts.length === 3) {
                let [month, day, year] = dateParts;

                // Handle different date formats and ensure correct order
                if (month.length === 4) {
                    // If month part has length 4 (e.g., yyyy-mm-dd format), swap the positions
                    [year, month] = [month, year];
                }

                // Format as MM/dd/yyyy
                return `${month.padStart(2, "0")}/${day.padStart(2, "0")}/${year}`;
            }

            // If invalid format, return an empty string or some error indication
            return "";
        }



        function Datepicker() {

            var datepickerUsed = false; // Flag to indicate if datepicker was used

            $('#txtMeetingDate').datepicker({

                autoclose: true,

                todayHighlight: true,

                startDate: '-0m',

                clearBtn: true,

                changeMonth: true,

                changeYear: true

            });


            $('#txtRevenueStartDate').datepicker({

