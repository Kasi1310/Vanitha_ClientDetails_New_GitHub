<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="frmClientReview.aspx.cs" Inherits="ClientDetails.frmClientReview" %>

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Client Review Form</title>
    <script src="Scripts/jquery-3.0.0.min.js"></script>
    <script src="Scripts/bootstrap.min.js"></script>
    <link href="Content/bootstrap.css" rel="stylesheet" />
    <script src="Scripts/Custom/Custom.js"></script>

    <link href="Assets/css/flatpickr.min.css" rel="stylesheet" />
    <link href="Assets/css/material_green.css" rel="stylesheet" />
    <script src="Assets/js/flatpickr.min.js"></script>
    <script src="Assets/js/ClientReviewFormJS/getClientReviewDetail.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <!-- CSS -->
      <style>
       /* [Previous CSS styles remain exactly the same] */
       body, html {
       height: 100%;
   }

   .border-10 {
       border: 3pt solid #009094;
       border-style: solid;
       border-width: 2px;
   }
   :root {
       --primary-color: #4361ee;
       --secondary-color: #3a0ca3;
       --accent-color: #f72585;
       --light-color: #f8f9fa;
       --medium-gray: #e9ecef;
       --dark-gray: #495057;
       --success-color: #4cc9f0;
       --warning-color: #f8961e;
       --info-color: #4895ef;
       --border-radius: 10px;
       --box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
   }

   body {
       font-family: Calibri;
       font-size: 16px;
       background: linear-gradient(135deg, #f5f7fa 0%, #e4e8f0 100%);
       padding: 20px;
       color: var(--dark-gray);
       line-height: 1;
       min-height: 100vh;
   }

   .form-container {
       max-width: 1200px;
       margin: 20px auto;
       background-color: white;
       padding: 30px 40px;
       border-radius: 10px;
       box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
       border: 1px solid rgba(255, 255, 255, 0.3);
   }



   .form-header {
       text-align: center;
       margin-bottom: 30px;
       padding-bottom: 15px;
       background: linear-gradient(to right, var(--primary-color), var(--secondary-color));
       color: white;
       padding: 20px;
       border-radius: var(--border-radius);
       box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
   }

   .form-header h1 {
       font-weight: 700;
       margin-bottom: 5px;
       letter-spacing: 0.5px;
       text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);
   }

   .section:hover {
       transform: translateY(-3px);
       box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);
   }

   

   #epcr-title{
       text-transform: none;
   }


   input[type="radio"]{
       cursor:pointer
   }

   .form-table {
       width: 100%;
       margin-bottom: 1rem;
       color: var(--dark-gray);
       border-collapse: separate;
       border-spacing: 0 12px;
   }

   .form-table td {
       padding: 15px 20px;
       vertical-align: middle;
   }

   .form-table tr {
       background-color: rgba(248, 249, 250, 0.7);
       transition: all 0.3s ease;
       border-radius: var(--border-radius);
       box-shadow: 0 2px 5px rgba(0, 0, 0, 0.03);
   }

   .form-table tr:hover {
       background-color: rgba(67, 97, 238, 0.05);
       box-shadow: 0 5px 15px rgba(67, 97, 238, 0.1);
   }

   .form-label {
       font-weight: 600;
       margin-bottom: 0px;
       color: rgb(93,103,112);
       white-space: normal;
       /*display: flex;
       align-items: center;*/        
       gap: 5px;
   }


   .flatpickr {
       background-color: rgba(255, 255, 255, 0.8);
   }

   .form-control {
       width: 100%;
       padding: 7px 10px 6px 6px;
       /*padding: 12px 15px;*/
       border: 2px solid #e2e8f0;
       border-radius: var(--border-radius);
       transition: all 0.3s ease;
       font-size: 0.95rem;
       background-color: rgba(255, 255, 255, 0.8);
   }

    .form-control-nontransport {
    width: 100%;
    padding: 6%;
    /*padding: 12px 15px;*/
    border: 2px solid #e2e8f0;
    border-radius: var(--border-radius);
    transition: all 0.3s ease;
    font-size: 0.95rem;
    background-color: rgba(255, 255, 255, 0.8);
}

   .form-control:focus {
       border-color: rgb(93,103,112);
       box-shadow: 0 0 0 3px rgba(67, 97, 238, 0.2);
       outline: none;
       background-color: white;
   }

   .radio-group {
       display: flex;
       gap: 25px;
       align-items: center;
   }

   .radio-option {
       display: flex;
       align-items: center;
       position: relative;
   }


   .form-check-label {
       font-weight: 500;
       color: var(--dark-gray);
       cursor: pointer;
       transition: color 0.2s ease;
   }

   .form-check-input:checked ~ .form-check-label {
       color: var(--secondary-color);
       font-weight: 600;
   }

   button{
       cursor:pointer
   }

   .btn-submit {
       padding: 14px 35px;
       font-weight: 600;
       letter-spacing: 0.5px;
      /* background: linear-gradient(to right, rgb(101,203,201), rgb(00,175,169));*/
       border: none;
       border-radius: 50px;
       transition: all 0.3s ease;
       font-size: 1rem;
       color: white;
       text-transform: uppercase;
       box-shadow: 0 4px 15px rgba(67, 97, 238, 0.3);
       position: relative;
       overflow: hidden;
   }

   .btn-submit:hover {
       transform: translateY(-3px);
       box-shadow: 0 8px 25px rgba(67, 97, 238, 0.4);
      background: linear-gradient(to right, rgb(00,175,169), rgb(101,203,201));
   }

   .btn-submit:active {
       transform: translateY(1px);
   }

   .validation-error {
       color: var(--accent-color);
       font-size: 0.85rem;
       margin-top: 5px;
       display: block;
       font-weight: 500;
       padding-left: 5px;
   }

   .input-group {
       display: flex;
       align-items: center;
       border-radius: var(--border-radius);
       overflow: hidden;
   }

   .input-group-text {
       display:none;
       min-width: 45px;
       justify-content: center;
       background-color: var(--primary-color);
       border: none;
       padding: 12px 15px;
       color: white;
       font-weight: 600;
   }

   .input-group .form-control {
       border-top-left-radius: 0;
       border-bottom-left-radius: 0;
       border-left: none;
   }

   .input-group-text:first-child {
       border-top-right-radius: 0;
       border-bottom-right-radius: 0;
   }

   .form-footer {
       text-align: center;
       margin-top: 40px;
       padding-top: 20px;
       border-top: 1px dashed var(--medium-gray);
   }

   .alert {
       padding: 15px 20px;
       margin-bottom: 20px;
       border-radius: var(--border-radius);
       border-left: 5px solid;
   }

   .alert-danger {
       background-color: #fff5f7;
       border-color: var(--accent-color);
       color: #721c24;
   }    

   .datepicker{
       background-color:#ffffff !important;
   }

   /* Responsive adjustments */
   @media (max-width: 768px) {
       .form-container {
           padding: 20px;
       }
       
       .form-table td {
           display: block;
           width: 100%;
           padding: 12px 15px;
       }
       
       .form-table tr {
           margin-bottom: 15px;
           display: block;
       }
       
       .radio-group {
           flex-direction: column;
           align-items: flex-start;
           gap: 12px;
       }
       
       .section {
           padding: 20px;
       }
       
       .form-label::before {
           display: none;
       }
       
       .section-title::after {
           display: none;
       }
   }
   .underline-textbox {
       border: none;
       border-bottom: 2px solid #000; /* Change color and thickness as needed */
       border-radius: 0;
       outline: none;
       box-shadow: none;
       background-color: transparent;
   }
   .underline-textbox:focus {
       border-bottom: 2px solid #007bff; /* or any color you prefer */
   }

   /* Animation for form loading */
   @keyframes fadeIn {
       from { opacity: 0; transform: translateY(20px); }
       to { opacity: 1; transform: translateY(0); }
   }

   .form-container {
       animation: fadeIn 0.6s ease-out forwards;
   }

   /* Hover effects for labels */
   .form-label:hover {
       color: rgb(93,103,112);
   }

   .form-label:hover::before {
       transform: scale(1.3);
   }

   </style>

</head>
<body>
    <input type="text" id="formType" runat="server" hidden value="" />
    <input type="text" id="zohoAccountId" runat="server" hidden value="" />
    <form id="clientReviewForm" runat="server">
        <div id="loader" style="display:none; position:fixed; top:0; left:0; right:0; bottom:0;background: rgba(0,0,0,0.5); z-index:9999; align-items:center; justify-content:center;">
            <div class="spinner-border text-light" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>

        <div class="form-container border-10">
            <div style="text-align:center;background: white;">
                <img src="data:image/jpg;base64," style="height:70px;width:auto" alt="Logo" />
            </div>
            <h1 class="text-center mb-4" style="font-family: Calibri; font-size: 40px; font-weight: bold; color: #009094; text-align: center;background: white;">CLIENT REVIEW</h1>

            <!-- MEETING DETAILS Section -->
            <div class="section" style="margin-bottom: 35px;padding: 25px;background-color: #fff;border-radius: 10px;box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1);border-left: 5px solid rgb(0,148,144);transition: transform 0.3s ease, box-shadow 0.3s ease;">
                <h3 class="section-title" style="font-weight: 700;color: rgb(0,175,169);margin-bottom: 25px;padding-bottom: 10px;border-bottom: 2px solid rgb(101,203,201);font-size: 1.3rem;display: flex;align-items: center;counter-increment: section;">MEETING DETAILS</h3>

                <table style="width: 100%; border-spacing: 0 10px;">
                    <!-- Row 1 -->
                    <tr style="text-align:center !important;">
                        <td style="display: inline-block; width: 8%;">
                            <div class="form-group">
                                <label class="form-label" for="clientNumber">CLIENT#:</label>
                                <asp:DropDownList ID="clientNumber" runat="server" CssClass="form-control search-dropdown" Style="text-align:center;">
                                    <asp:ListItem Text="SELECT" Value="" />
                                </asp:DropDownList>
                                <span id="frvclientNumber" style="color:Red; display:none;">CLIENT# IS REQUIRED</span>
                            </div>
                        </td>

                        <td style="display: inline-block; width: 46%;">
                            <div class="form-group">
                                <label class="form-label" for="clientName">CLIENT NAME:</label>
                                <input type="text" id="clientName" name="clientName" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display: inline-block; width: 46%;">
                            <div class="form-group">
                                <label class="form-label" for="meetingDate">MEETING OR SENT DATE:</label>
                                <input type="text" id="meetingDate" name="meetingDate" class="form-control datepicker" style="text-align:center;" />
                                <span id="rfvMeetingDate" style="color: Red; display: none;">MEETING OR SENT DATE IS REQUIRED</span>
                            </div>
                        </td>
    
                    </tr>

                    <!-- Row 2 -->
                    <tr style="text-align:center !important;">
                        <td style="display: inline-block; width: 33.33%;">
                            <div class="form-group">
                                <label class="form-label" for="accountExecutive">ACCOUNT EXECUTIVE:</label>
                                <input type="text" id="accountExecutive" name="accountExecutive" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display: inline-block; width: 33.33%;">
                            <div class="form-group">
                                <label class="form-label" for="aeEmail">EMAIL:</label>
                                <input type="email" id="aeEmail" name="aeEmail" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display: inline-block; width: 33.33%;">
                            <div class="form-group">
                                <label class="form-label" for="aePhone">PHONE #:</label>
                                <input type="text" id="aePhone" name="aePhone" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                    </tr>


                    <!-- Row 3 -->
                    <tr style="text-align:center !important;">
                        <table style="width: 100%;">
                            <tbody id="recipientContainer">
                                <!-- First Row (with labels) -->
                                <tr class="recipientRow" style="text-align:center;">
                                    <td style="display: inline-block; width: 33.33%; vertical-align: top;">
                                        <div class="form-group">
                                            <label class="form-label">RECIPIENT RECEIVING NAME:</label>
                                            <input type="text" name="recipientReceivingName" class="form-control" style="text-align:center;"/>
                                            <span style="color: Red; display: none;">
                                                RECIPIENT RECEIVING NAME IS REQUIRED
                                            </span>
                                        </div>
                                    </td>
                                    <td style="display: inline-block; width: 33.33%; vertical-align: top;">
                                        <div class="form-group">
                                            <label class="form-label">RECIPIENT RECEIVING TITLE:</label>
                                            <input type="text" name="recipientReceivingTitle" class="form-control" style="text-align:center;" />
                                            <span style="color: Red; display: none;">
                                                RECIPIENT RECEIVING TITLE IS REQUIRED
                                            </span>
                                        </div>
                                    </td>
                                    <td style="display: inline-block; width: 33.33%; vertical-align: top;">
                                        <div class="form-group">
                                            <label class="form-label">RECIPIENT RECEIVING EMAIL:</label>
                                            <input type="email" name="recipientReceivingEmail" class="form-control" style="text-align:center;" />
                                            <span style="color: Red; display: none;">
                                                RECIPIENT RECEIVING EMAIL IS REQUIRED
                                            </span>
                                            <div class="recipientButtons pdf-exclude" style="text-align: right; margin-top: 5px;">
                                                <button type="button" class="btn btn-success addRecipient">+</button>
                                            </div>
                                        </div>

                                        
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </tr>

                </table>
            </div>

            <!-- PERFORMANCE METRICS Section -->
            <div class="section" style="margin-bottom: 35px; padding: 25px; background-color: #fff; border-radius: 10px; box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1); border-left: 5px solid rgb(0, 148, 144); transition: transform 0.3s ease, box-shadow 0.3s ease;">
                <h3 class="section-title" style="font-weight: 700; color: rgb(0, 175, 169); margin-bottom: 25px; padding-bottom: 10px; border-bottom: 2px solid rgb(101, 203, 201); font-size: 1.3rem; display: flex; align-items: center; counter-increment: section;">
                    PERFORMANCE METRICS
                </h3>

                <table style="width: 100%; border-spacing: 0 10px;">
                    <!-- Row 1 -->
                    <tr style="text-align:center !important;">
                        <td style="display: inline-block; width: 33.33%;">
                            <div class="form-group">
                                <label class="form-label" for="clientRevenueNumberStartDate">CLIENT REVENUE NUMBERS - START DATE:</label>
                                <input type="text" id="clientRevenueNumberStartDate" name="clientRevenueNumberStartDate" class="form-control datepicker" style="text-align:center;" />
                                <span id="frvClientRevenueNumberStartDate" style="color: Red; display: none;">CLIENT REVENUE NUMBERS - START DATE IS REQUIRED</span>
                            </div>
                        </td>
                        <td style="display: inline-block; width: 33.33%;">
                            <div class="form-group">
                                <label class="form-label" for="clientRevenueNumberEndDate">CLIENT REVENUE NUMBERS - END DATE:</label>
                                <input type="text" id="clientRevenueNumberEndDate" name="clientRevenueNumberEndDate" class="form-control datepicker" style="text-align:center;" />
                                <span id="frvClientRevenueNumberEndDate" style="color: Red; display: none;">CLIENT REVENUE NUMBERS - END DATE IS REQUIRED</span>
                            </div>
                        </td>
                        <td style="display: inline-block; width: 33.33%;">
                            <div class="form-group">
                                <label class="form-label" for="reportType">REPORT TYPE</label>
                                <input type="text" id="reportType" name="reportType" class="form-control" value="Date of Entry" disabled style="text-align:center;" />
                            </div>
                        </td>
                    </tr>

                    <!-- Row 2 -->
                    <tr style="line-height:1 !important; text-align:center;">
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="transports">TRANSPORTS:</label>
                                <input type="text" id="transports" name="transports" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td hidden style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="ytdRevenue">YTD REVENUE:</label>
                                <input type="text" id="ytdRevenue" name="ytdRevenue" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:10% !important;" for="revenuePerTransport">REVENUE PER TRANSPORT:</label>
                                <input type="text" id="revenuePerTransport" name="revenuePerTransport" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="charges">CHARGES:</label>
                                <input type="text" id="charges" name="charges" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="payments">PAYMENTS:</label>
                                <input type="text" id="payments" name="payments" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="adjustments">ADJUSTMENTS:</label>
                                <input type="text" id="adjustments" name="adjustments" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="writeOffs">WRITE-OFFS:</label>
                                <input type="text" id="writeOffs" name="writeOffs" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 14.28%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" style="padding-top:20% !important;" for="collectionRate">COLL RATE %:</label>
                                <input type="text" id="collectionRate" name="collectionRate" class="form-control" style="text-align:center;" />
                            </div>
                        </td>
                        <td style="display:inline-block; width: 50%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" for="clientComments">CLIENT COMMENTS:</label>
                                <textarea id="clientComments" name="clientComments" class="form-control" style="text-align:center;"></textarea>
                            </div>
                        </td>
                        <td style="display:inline-block; width: 50%; vertical-align: top;">
                            <div class="form-group">
                                <label class="form-label" for="aeComments">AE COMMENTS:</label>
                                <textarea id="aeComments" name="aeComments" class="form-control" style="text-align:center;"></textarea>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>

            <!-- CHARGE RATES Section -->
            <div class="section" style="margin-bottom: 35px; padding: 25px; background-color: #fff; border-radius: 10px; box-shadow: 0 8px 25px rgba(0, 0, 0, 0.1); border-left: 5px solid rgb(0,148,144); transition: transform 0.3s ease, box-shadow 0.3s ease;">
              <h3 class="section-title" style="font-weight: 700; color: rgb(0,175,169); margin-bottom: 25px; padding-bottom: 10px; border-bottom: 2px solid rgb(101,203,201); font-size: 1.3rem; display: flex; align-items: center;">
                YOUR CHARGE RATES
              </h3>

              <table style="width: 100%; border-spacing: 0 10px;">
                <!-- Row 1 -->
                <tr style="text-align:center;">
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 10%;">DATE OF LAST RATE CHANGE:</label>
                    <input type="text" id="dateOfLastRateChange"  class="form-control datepicker" style="width: 100%; text-align:center;" />
                  </td>
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 22%;">BLS:</label>
                    <input type="text" id="bls" class="form-control" step="0.01" style="width: 100%; text-align:center;" />
                  </td>
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 22%;">BLS NE:</label>
                    <input type="text" id="blsNe" class="form-control" step="0.01" style="width: 100%; text-align:center;" />
                  </td>
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 22%;">ALS:</label>
                    <input type="text" id="als" class="form-control" step="0.01" style="width: 100%; text-align:center;" />
                  </td>
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 22%;">ALS NE:</label>
                    <input type="text" id="alsNe" class="form-control" step="0.01" style="width: 100%; text-align:center;" />
                  </td>
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 22%;">ALS2:</label>
                    <input type="text" id="als2" class="form-control" step="0.01" style="width: 100%; text-align:center;" />
                  </td>
                  <td style="width: 12.5%; vertical-align: top;">
                    <label class="form-label" style="display:block; padding-top: 22%;">MILEAGE:</label>
                    <input type="text" id="mileage" class="form-control" step="0.01" style="width: 100%; text-align:center;" />
                  </td>

                 <td style="width: 12.5%; vertical-align: top; padding-top: 3%; text-align: center;">
                  <label class="form-label" style="display: block; margin-bottom: 5px;">
                    NON-TRANSPORT:
                  </label>
                  <span style="display: inline-block; margin-right: 10px;">
                    <input type="radio" name="nonTransport" id="nonTransortYes" value="yes" />
                    <label for="nonTransportYes" style="cursor:pointer;">Yes</label>
                  </span>
                  <span style="display: inline-block;">
                    <input type="radio" name="nonTransport" id="nonTransportNo" value="no" checked />
                    <label for="nonTransportNo" style="cursor:pointer;">No</label>
                  </span>
                </td>
                </tr>

                <!-- Hidden Non-Transport Comment Row -->
