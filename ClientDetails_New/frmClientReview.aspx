        function reviewDateCalculator(selectedValue) {
            const meetDateStr = $("#meetingDate").val();

            if (!meetDateStr || !selectedValue) {
                $("#nextReviewDate").val('');
                return;
            }

            // Parse MM/DD/YYYY string to Date object
            const parts = meetDateStr.split('/');
            if (parts.length !== 3) {
                $("#nextReviewDate").val('');
                return;
            }

            const month = parseInt(parts[0], 10) - 1; // Month is 0-based
            const day = parseInt(parts[1], 10);
            const year = parseInt(parts[2], 10);

            const meetDate = new Date(year, month, day);

            if (isNaN(meetDate)) {
                $("#nextReviewDate").val('');
                return;
            }

            let nextReviewDate = new Date(meetDate); // clone

            // Add interval
            if (selectedValue.trim().toUpperCase() === 'YEARLY') {
                nextReviewDate.setFullYear(nextReviewDate.getFullYear() + 1);
            } else if (selectedValue.trim().toUpperCase() === 'QUARTERLY') {
                nextReviewDate.setMonth(nextReviewDate.getMonth() + 3);
            } else {
                nextReviewDate.setMonth(nextReviewDate.getMonth() + 6);
            }

            // Format back to MM/DD/YYYY
            const formatted = (nextReviewDate.getMonth() + 1).toString().padStart(2, '0') + '/' +
                nextReviewDate.getDate().toString().padStart(2, '0') + '/' +
                nextReviewDate.getFullYear();

            $("#nextReviewDate").val(formatted);
        }

        function get12MonthRange() {
            const meetingDateStr = $("#meetingDate").val();

            if (!meetingDateStr) {
                $("#clientRevenueNumberStartDate").val('');
                $("#clientRevenueNumberEndDate").val('');
                return;
            }

            // Parse MM/DD/YYYY
            const parts = meetingDateStr.split('/');
            if (parts.length !== 3) {
                $("#clientRevenueNumberStartDate").val('');
                $("#clientRevenueNumberEndDate").val('');
                return;
            }

            const month = parseInt(parts[0], 10) - 1; // 0-based
            const day = parseInt(parts[1], 10);
            const year = parseInt(parts[2], 10);

            const meetingDate = new Date(year, month, day);
            if (isNaN(meetingDate)) {
                $("#clientRevenueNumberStartDate").val('');
                $("#clientRevenueNumberEndDate").val('');
                return;
            }

            const startDate = new Date(year - 1, month, 1);

            const endDate = new Date(year, month, 0); // 0th day of meeting month = last day of previous month

            // Format MM/DD/YYYY
            const formatDate = (date) => {
                const mm = (date.getMonth() + 1).toString().padStart(2, '0');
                const dd = date.getDate().toString().padStart(2, '0');
                const yyyy = date.getFullYear();
                return `${mm}/${dd}/${yyyy}`;
            };

            // Populate the fields
            $("#clientRevenueNumberStartDate").val(formatDate(startDate));
            $("#clientRevenueNumberEndDate").val(formatDate(endDate));
            $("#clientRevenueNumberEndDate").trigger('change');
        }

        function generateXmlFromForm() {
            let xml = '<ClientReview>';
            const processedRadioNames = {};

            const zohoAccountId = $("#zohoAccountId").val();
            xml += `<zohoAccountId>${escapeXml(zohoAccountId)}</zohoAccountId>`;

            const formType = $("#formType").val();
            xml += `<cReviewFormType>${escapeXml(formType)}</cReviewFormType>`;

            // Process all inputs except recipients
            $('#clientReviewForm').find('input, select, textarea').each(function () {
                const name = this.name || this.id;
                if (!name || name.startsWith('recipientReceiving')) return;

                let val = '';

                if (this.type === 'radio') {
                    if (processedRadioNames[name]) return;
                    const checkedRadio = $(`input[name="${name}"]:checked`, '#clientReviewForm');
                    if (checkedRadio.length) {
                        val = escapeXml(checkedRadio.val());
                        xml += `<${name}>${val}</${name}>`;
                    }
                    processedRadioNames[name] = true;
                } else {
                    val = escapeXml(this.value || '');
                    xml += `<${name}>${val}</${name}>`;
                }
            });

            // Recipients grouped in <Recipients>
            xml += '<recipients>';
            $('#recipientContainer .recipientRow').each(function () {
                const name = escapeXml($(this).find('input[name="recipientReceivingName"]').val() || '');
                const title = escapeXml($(this).find('input[name="recipientReceivingTitle"]').val() || '');
                const email = escapeXml($(this).find('input[name="recipientReceivingEmail"]').val() || '');

                xml += `
            <recipient>
                <recipientReceivingName>${name}</recipientReceivingName>
                <recipientReceivingTitle>${title}</recipientReceivingTitle>
                <recipientReceivingEmail>${email}</recipientReceivingEmail>
            </recipient>
        `;
            });
            xml += '</recipients>';

            xml += '</ClientReview>';
            return xml;
        }

        function escapeXml(str) {
            return String(str || '').replace(/[<>&'"]/g, function (c) {
                return {
                    '<': '&lt;',
                    '>': '&gt;',
                    '&': '&amp;',
                    '\'': '&apos;',
                    '"': '&quot;'
                }[c];
            });
        }



