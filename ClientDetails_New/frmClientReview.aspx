            });

            $('#txtDOSE').datepicker({

                autoclose: true,

                todayHighlight: true,

                startDate: '-0m',

                clearBtn: true,

                changeMonth: true,

                changeYear: true

            });

            $('.datepicker-autoclose').inputmask("99-99-9999");

            $('.datepicker-autoclose').on('keydown', function (event) {

                // Allow Backspace key

                if (event.keyCode === 8) {

                    $(this).val('');

                    return;

                }

                // Prevent default action for other keys (Inputmask handles allowed keys)

                if (!/^[0-9/]$/.test(event.key) && event.keyCode !== 8) {

                    event.preventDefault();

                }

            });

            $('.datepicker-autoclose').on('blur', function () {

                var dateStr = $(this).val();

                if (dateStr === "") {

                    return;

                }

                if (!isValidDate(dateStr)) {

                    console.log(dateStr)

                    swal("Info", "Invalid date. Please enter a valid date in mm-dd-yyyy format.", "info");

                    $(this).val('');

                }

            });

        }

        function isValidDate(dateStr) {

            if (!dateStr || dateStr.length !== 10) {

                return false;

            }

            var parts = dateStr.split('-');

            var month = parseInt(parts[0], 10);

            var day = parseInt(parts[1], 10);

            var year = parseInt(parts[2], 10);

            // Check month

            if (month < 1 || month > 12) {

                return false;

            }

            // Check day

            var maxDays = [31, (isLeapYear(year) ? 29 : 28), 31, 30, 31, 30, 31, 31, 30, 31, 30, 31];

            if (day < 1 || day > maxDays[month - 1]) {

                return false;

            }

            // Check year (optional, but you can set range if needed)

            if (year < 1000 || year > 9999) {

                return false;

            }

            return true;

        }

        function isLeapYear(year) {

            return (year % 4 === 0 && year % 100 !== 0) || (year % 400 === 0);

        }
       
   

